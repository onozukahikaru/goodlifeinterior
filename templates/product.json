{
  "sections": {
    "main": {
      "type": "main-product",
      "blocks": {
        "product_meta": {
          "type": "product_meta",
          "settings": {
            "show_vendor": false,
            "show_sku": false,
            "show_reviews_badge": false,
            "show_share_buttons": false
          }
        },
        "variant_selector": {
          "type": "variant_selector",
          "settings": {
            "selector_mode": "block",
            "color_mode": "block",
            "show_quantity_selector": true,
            "show_taxes_included": false,
            "show_inventory_quantity": false,
            "low_inventory_threshold": 0
          }
        },
        "buy_buttons": {
          "type": "buy_buttons",
          "settings": {
            "show_payment_button": true
          }
        },
        "description": {
          "type": "description",
          "settings": {
            "display_mode": "show_all"
          }
        },
        "6f3520b1-6803-4783-8c8b-bb95874e3d14": {
          "type": "liquid",
          "settings": {
            "title": "動画",
            "liquid": "{% assign media = product.metafields.custom._video.value %}\n{% if media.media_type == 'video' %}\n  {{ media | video_tag: image_size: '300x', controls: true, autoplay: false, width:300, height:auto }}\n{% endif %}",
            "display_mode": "show_all"
          }
        }
      },
      "block_order": [
        "product_meta",
        "variant_selector",
        "buy_buttons",
        "description",
        "6f3520b1-6803-4783-8c8b-bb95874e3d14"
      ],
      "settings": {
        "image_size": "small",
        "enable_video_looping": false,
        "enable_image_zoom": true,
        "zoom_effect": "outside",
        "carousel_effect": "fade"
      }
    },
    "product-recommendations": {
      "type": "product-recommendations",
      "settings": {
        "heading": "You may also like",
        "show_quick_buy": false
      }
    },
    "recently-viewed-products": {
      "type": "recently-viewed-products",
      "settings": {
        "title": "Recently viewed",
        "show_quick_buy": false
      }
    },
    "768ec9bb-aa5f-46c7-8a94-c9e95bdb5f2e": {
      "type": "custom-liquid",
      "settings": {
        "title": "",
        "include_margins": false,
        "liquid": "{%- assign collection_titles = product.collections | map: 'title' -%}\n\n{% for first_level_link in linklists.main-menu.links %}\n  {% if collection_titles contains first_level_link.title %}\n    <div>\n      <a href=\"\/\">ホーム<\/a>\n      > <a href=\"{{ first_level_link.url }}\">{{ first_level_link.title }}<\/a>\n\n      {% for second_level_link in first_level_link.links %}\n        {% if collection_titles contains second_level_link.title %}\n          > <a href=\"{{ scond_level_link.url }}\">{{ second_level_link.title }}<\/a>\n        {% endif %}\n      {% endfor %}\n\n    <\/div>\n  {% endif %}\n{% endfor %}"
      }
    }
  },
  "order": [
    "main",
    "product-recommendations",
    "recently-viewed-products",
    "768ec9bb-aa5f-46c7-8a94-c9e95bdb5f2e"
  ]
}
